{
  "expo": {
    "name": "Smart_Study_Planner",
    "slug": "Smart_Study_Planner",
    "scheme": "Smart_Study_Planner",        
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": ["@react-native-google-signin/google-signin", "expo-router"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff",
        "googleServicesFile": "./google-services.json",
        "permissions": ["INTERNET"]
      },
      "package": "com.Linfel.Smart_Study_Planner"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "ade8da0e-cc47-4081-a63b-723f63c227d5"
      }
    }
  }
}
